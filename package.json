{
  "name": "Agile-Kanban-Planning-Application-(AKPA)",
  "version": "1.0.0",
  "description": "Web application to better plan Agile/Kanban projects",
  "main": "index.html",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "gulp clean",
    "pretest": "gulp compile-test",
    "jest": "node ./packages/jest-cli/bin/jest.js",
    "test": "jest",
    "preclient": "gulp webpack",
    "client": "gulp webserver",
    "preserver": "gulp compile-server",
    "server": "node ./dist/server/server.js"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/tests/Results",
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testRegex": "/tests/JsOutput/tests/.*\\.test\\.jsx?$"
  },
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/google.analytics": "0.0.29",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/stripe": "^2.0.2",
    "@types/winston": "^2.3.0",
    "body-parser": "^1.17.1",
    "chart-library": "^1.1.0",
    "express": "^4.15.2",
    "express-formidable": "^1.0.0",
    "formidable": "^1.1.1",
    "lokijs": "^1.4.3",
    "multer": "^1.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-easy-chart": "^0.1.12",
    "stripe": "^4.18.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/d3": "^4.4.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "d3": "^3.5.17",
    "d3-array": "^0.8.1",
    "d3-shape": "^0.7.1",
    "d3-time-format": "^0.4.0",
    "del": "^2.2.2",
    "express-mock": "^0.3.0",
    "fbjs": "^0.8.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-express": "^0.3.5",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-typescript": "^3.1.3",
    "gulp-util": "^3.0.8",
    "gulp-webpack": "^1.5.0",
    "gulp-webserver": "^0.9.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.0.0",
    "material-ui": "^0.16.7",
